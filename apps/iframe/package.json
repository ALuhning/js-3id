{
  "name": "3id-iframe",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "build:prod": "webpack --config webpack.config.js --mode=production",
    "build:clay": "webpack --config webpack.config.js --mode=production --output-path ../../public-clay --env CERAMIC_ENV=clay && cp ../../public/index.html ../../public-clay/index.html",
    "build:dev": "npm run build:prod -- --env CERAMIC_ENV=develop",
    "build:test": "npm run build:prod -- --env CERAMIC_ENV=test",
    "dev:build": "webpack --config webpack.config.js --mode=development",
    "dev:server": "http-server -c-1 -p 30001 ../../public",
    "start": "run-p dev:**",
    "start:testnet": "CERAMIC_API=https://ceramic-clay.3boxlabs.com npm run start"
  },
  "dependencies": {
    "3id-did-provider": "^1.1.0",
    "@3id/common": "^0.2.1",
    "@3id/connect-display": "^0.2.1",
    "@3id/manager": "^0.2.1",
    "@3id/ui-provider": "^0.2.0",
    "@3id/window-auth-provider": "^0.2.1",
    "@babel/runtime": "7.15.4",
    "@ceramicnetwork/http-client": "^1.2.2",
    "@ceramicstudio/idx": "^0.12.2",
    "errors-utils": "^0.1.0",
    "postmsg-rpc": "^2.4.0",
    "rpc-utils": "^0.4.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.12.13",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "css-loader": "^5.2.6",
    "dids": "2.4.0",
    "icss-replace-symbols": "^1.1.0",
    "node-sass": "^6.0.0",
    "sass-loader": "^11.1.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^4.0.0",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0"
  }
